WithEmailAndPassword = async (email, password) => {\n  if(!email || !password) return;\n\n  return await createUserWithEmailAndPassword(auth, email, password);\n}\n\nexport const signInAuthUserWithEmailAndPassword = async (email, password) => {\n  if(!email || !password) return;\n\n  return await signInWithEmailAndPassword(auth, email, password);\n}\n\nexport const signOutUser = async () => await signOut(auth);\n\nexport const onAuthStateChangedListener = (callback) => \n  onAuthStateChanged(auth, callback);\n\n","/home/aram/Desktop/React-file/crwn-clothing/src/components/sign-up-form/sign-up-form.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/form-input/form-input.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/button/button.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/routes/authentication/authentication.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/sign-in-form/sign-in-form.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/contexts/user.context.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/routes/shop/shop.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/product-card/product-card.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/cart-icon/cart-icon.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/cart-dropdown/cart-dropdown.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/contexts/cart.context.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/cart-item/cart-item.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/routes/checkout/checkout.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/components/checkout-item/checkout-item.component.jsx",[],[],"/home/aram/Desktop/React-file/crwn-clothing/src/shop-data.js",[],[],"/home/aram/Desktop/